    "dev": "next dev -H 0.0.0.0 -p 3000",
    "build": "next build",
    "build:memory-optimized": "./scripts/memory-optimized-build.sh",
    "build:incremental": "next build --experimental-build-mode=compile",
    "start": "next start",
    "lint": "next lint",
    "test:compile": "tsc --project tsconfig.test.json",
    "test:validate": "npm run test:compile && node dist/lib/validations/cuid.js",
    "test:cuid": "npm run test:compile && node -e \"const {isValidCUID, analyzeCUID} = require('./dist/lib/validations/cuid'); console.log('Legacy CUID test:', isValidCUID('cm59coeho001etlext1ozmo')); console.log('Current CUID test:', isValidCUID('cmgu3bf1s000s78lth2hxr5uy'));\"",
    "validate:system": "npm run test:cuid && echo 'VAL-02 AUTOMATED: CUID validation system verified'",
    "cleanup": "./scripts/memory-cleanup.sh",
    "cleanup:memory": "tsx scripts/memory-monitor-test.ts",
    "memory:assess": "tsx scripts/memory-assessment.ts",
    "optimize": "npm run cleanup && npm run build:memory-optimized"
  },
  "prisma": {
    "seed": "tsx --require dotenv/config scripts/seed.ts"
